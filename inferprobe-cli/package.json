# inferprobe-cli/package.json
# InferProbe CLI Tool for CI/CD integration

{
  "name": "inferprobe-cli",
  "version": "1.0.0",
  "description": "CLI tool for InferProbe AI testing in CI/CD pipelines",
  "main": "dist/index.js",
  "bin": {
    "inferprobe": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:ai": "inferprobe scan batch --dataset ./test-data.json --endpoint-id $ENDPOINT_ID",
    "test:safety": "inferprobe test adversarial --fail-on-critical",
    "analyze:costs": "inferprobe cost analyze --model gpt-4-turbo",
    "ci:full": "npm run test:ai && npm run test:safety && npm run analyze:costs"
  },
  "keywords": ["ai", "testing", "cicd", "ml", "safety"],
  "author": "InferProbe Team",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.0.0",
    "axios": "^1.6.0",
    "chalk": "^5.3.0",
    "ora": "^7.0.0"
  }
}
